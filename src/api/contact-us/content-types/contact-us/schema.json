{
  "kind": "singleType",
  "collectionName": "contact_uses",
  "info": {
    "singularName": "contact-us",
    "pluralName": "contact-uses",
    "displayName": "contact-us",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "contactForm": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "shared.form-field"
      ]
    },
    "contacts_infos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contacts-info.contacts-info"
    },
    "formActions": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.advanced-button"
    },
    "contactBoxHeading": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "contactBoxImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seoTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "seoDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "whatsapp": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::social-media.social-media"
    },
    "formSendErrorMessage": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "formSendSuccessMessage": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    }
  }
}
